    <div class="box" id="heading">
        <h1><%= listTitle  %></h1>
  
    </div>

    <div class="box">
      
       <% for(var i=0;i<NewListItems.length;i++) { %>
           <div class="item">
       <input type="checkbox"> <p><%= NewListItems[i] %></p></div>
        <% } %>
        <form class="" action="/" method="post">
            <div class="input-group mb-3">
             <input name="NewItem" type="text" class="form-control" placeholder="ADD Task"  autocomplete="off">
             <div>
               <button type="submit" name="list" value= <%= listTitle %> >+</button>
             </div>
           </div>
          </form>
         </div> 
    
</div>




